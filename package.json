{
  "name": "funcs",
  "version": "0.0.1",
  "description": "A PoC looking at cloud function tools",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug:func1": "node --inspect node_modules/.bin/functions-framework --target=listGoogleAPIs",
    "debug:func2": "node --inspect node_modules/.bin/functions-framework --target=helloPubSub --signature-type=event",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "start": "functions-framework --source=build/src/ --target=helloWorld",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^2.1.1",
    "@types/express": "^4.17.13",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "gts": "^3.1.0",
    "typescript": "^4.0.3"
  }
}
